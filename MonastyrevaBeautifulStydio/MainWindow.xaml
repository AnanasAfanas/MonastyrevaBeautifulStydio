<Window x:Class="MonastyrevaBeautifulStydio.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MonastyrevaBeautifulStydio"
        mc:Ignorable="d"
        Title="MainWindow" Height="800" Width="1300">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width=".40*" />
            <ColumnDefinition Width="1*" />
            <ColumnDefinition Width="1*" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*" />
            <RowDefinition Height="1*" />
            <RowDefinition Height="1*" />
            <RowDefinition Height="1*" />
            <RowDefinition Height="1*" />
        </Grid.RowDefinitions>
        <Frame x:Name="MainFrame" Grid.Column="1"
               Grid.RowSpan="5"
               Grid.ColumnSpan="2">
            <Frame.Background>
                <ImageBrush ImageSource="/Resources/photo_2022-11-09_11-45-45.jpg"/>
            </Frame.Background>

        </Frame>
        <Frame x:Name="ButtonsFrm" Grid.RowSpan="5">
            <Frame.Background>
                <ImageBrush ImageSource="/Resources/photo_2022-11-09_11-45-36.jpg"/>
            </Frame.Background>
        </Frame>
    </Grid>
</Window>
